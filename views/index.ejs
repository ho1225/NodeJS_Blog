<div class="author">
    <h1 class="author__heading">
        Hi, I am Sean.
    </h1>
    <p class="author__body">
        Software developer
    </p>
</div>

<img src="/img/hero-image.webp" alt="person looking out through" class="hero-image" width="981" height="528">

<section class="articles">
    <h2 class="articles__heading">Latest Posts</h2>
    <ul class="article-ul">
        <% data.forEach(post => { %>
            <li>
                <a href="#">
                    <span>
                        <%= post.title %>
                    </span>
                    <span class="article-list__date">
                        <%= post.createdAt %>
                    </span>
                </a>
            </li>
        <% }) %>
    </ul>

    <a href="#" clas="pagination">
        &lt; View Older Posts
    </a>
</section>